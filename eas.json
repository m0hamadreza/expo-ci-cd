{
  "cli": {
    "version": ">= 7.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "build-and-maestro-test": {
      "withoutCredentials": true,
      "android": {
        "config": "build-and-maestro-test.yml",
        "buildType": "apk",
        "image": "latest"
      },
      "ios": {
        "config": "build-and-maestro-test-ios.yml",
        "simulator": true,
        "image": "latest"
      },
      "channel": "build-and-maestro-test"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_ENV":"PRODUCTION"
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "production"
    },
    "stage": {
      "env": {
        "EXPO_PUBLIC_ENV":"STAGE"
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "stage"
    }
  },
  "submit": {
    "production": {}
  }
}
